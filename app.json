{
  "expo": {
    "name": "PSS Mobile",
    "slug": "pss-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alphasistemas.pssmobile",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.alphasistemas.pssmobile",
      "permissions": [
        "android.permission.VIBRATE",
        "android.permission.POST_NOTIFICATIONS"
      ]
    },
    "web": {},
    "scheme": "pss-mobile",
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "color": "#002B5B",
          "enableBackgroundRemoteNotifications": true
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8e78dc61-4cc9-45bf-a379-094ae2988f83"
      }
    }
  }
}
